{
    "root": true,
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "extends": [
      "@nuxtjs",
      "plugin:prettier/recommended",
      "plugin:vue/recommended",

    ],
    "rules":{
      "vue/html-self-closing": ["off", {
        "html": {
          "void": "never",
          "normal": "never",
          "component": "never"
        }
      }]
    }
    
   
    
  
  }